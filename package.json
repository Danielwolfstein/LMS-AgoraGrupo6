  },
  "eslintConfig": {
    "ignorePatterns": "build",
    "extends": "./node_modules/standard/eslintrc.json",
    "env": {
      "cypress/globals": true,
      "jest": true
    },
    "plugins": [
      "cypress"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "eslint-plugin-cypress": "2.12.1",
    "standard": "17.0.0"
  },
  "keywords": [],
