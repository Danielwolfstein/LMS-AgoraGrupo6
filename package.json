{
  "name": "agora",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "engines": {
    "npm": "9.4.1"
  },
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true npm run build --workspace=frontend",
